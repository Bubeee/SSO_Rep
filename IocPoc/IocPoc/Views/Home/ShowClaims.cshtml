@model System.Collections.Generic.IEnumerable<System.Security.Claims.Claim>

@{
    ViewBag.Title = "title";
}

<h2>Claims</h2>

<table class="table-bordered">
    <tr>
        <th>Claim Type</th>
        <th>Claim Value</th>
    </tr>
    @foreach (var claim in Model)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>